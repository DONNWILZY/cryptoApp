{
    "version": 2,
    "name": "cryptoApp",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],

    "crons": [{
      "path": "/cron",
      "schedule": "0 10 * * *"
    }]
    
  }


  